{
  "name": "terminal-control",
  "displayName": "Terminal Control",
  "description": "Use shortcuts to control and focus your terminals",
  "version": "0.0.1",
  "repository": "https://github.com/pcandido/terminal-control",
  "license": "MIT",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "terminal-control.add",
        "key": "ctrl+shift+numpad_Add",
        "mac": "ctrl+shift+numpad_Add"
      },
      {
        "command": "terminal-control.close",
        "key": "ctrl+shift+numpad_Subtract",
        "mac": "ctrl+shift+numpad_Subtract"
      },
      {
        "command": "terminal-control.hide",
        "key": "ctrl+shift+numpad0",
        "mac": "ctrl+shift+numpad0"
      },
      {
        "command": "terminal-control.focus-1",
        "key": "ctrl+shift+numpad1",
        "mac": "ctrl+shift+numpad1"
      },
      {
        "command": "terminal-control.focus-2",
        "key": "ctrl+shift+numpad2",
        "mac": "ctrl+shift+numpad2"
      },
      {
        "command": "terminal-control.focus-3",
        "key": "ctrl+shift+numpad3",
        "mac": "ctrl+shift+numpad3"
      },
      {
        "command": "terminal-control.focus-4",
        "key": "ctrl+shift+numpad4",
        "mac": "ctrl+shift+numpad4"
      },
      {
        "command": "terminal-control.focus-5",
        "key": "ctrl+shift+numpad5",
        "mac": "ctrl+shift+numpad5"
      },
      {
        "command": "terminal-control.focus-6",
        "key": "ctrl+shift+numpad6",
        "mac": "ctrl+shift+numpad6"
      },
      {
        "command": "terminal-control.focus-7",
        "key": "ctrl+shift+numpad7",
        "mac": "ctrl+shift+numpad7"
      },
      {
        "command": "terminal-control.focus-8",
        "key": "ctrl+shift+numpad8",
        "mac": "ctrl+shift+numpad8"
      },
      {
        "command": "terminal-control.focus-9",
        "key": "ctrl+shift+numpad9",
        "mac": "ctrl+shift+numpad9"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.81.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}